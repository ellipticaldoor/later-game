{
  "name": "later",
  "version": "1.0.0",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "client": "cross-env NODE_ENV=development parcel index.html --no-autoinstall --no-hmr",
    "client:watch": "cross-env NODE_ENV=development parcel index.html --no-autoinstall",
    "client:no-cache": "cross-env NODE_ENV=development parcel index.html --no-autoinstall --no-hmr --no-cache",
    "build": "rimraf build && cross-env NODE_ENV=production parcel build index.html --no-source-maps --out-dir build",
    "test": "cross-env NODE_ENV=development jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:coverage": "cross-env NODE_ENV=development jest --coverage",
    "test:coverage:all": "cross-env NODE_ENV=development COVERAGE=all jest --coverage",
    "test:report": "http-server -s -o -c-1 -p 8888 coverage/lcov-report"
  },
  "dependencies": {
    "matter-js": "^0.14.1",
    "mousetrap": "^1.6.1",
    "pixi.js": "^5.0.0-alpha.2",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/matter-js": "^0.10.0",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^10.1.2",
    "@types/ramda": "^0.25.29",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.6",
    "http-server": "^0.11.1",
    "jest": "^22.4.4",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.8.1",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">= 9.4.0",
    "npm": ">= 5.8.0"
  }
}
