{
  "name": "later",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn server\" \"yarn client\"",
    "client": "cross-env NODE_ENV=development parcel src/client/index.html --no-autoinstall --no-hmr",
    "client:reload": "cross-env NODE_ENV=development parcel src/client/index.html --no-autoinstall",
    "client:no-cache": "cross-env NODE_ENV=development parcel src/client/index.html --no-autoinstall --no-hmr --no-cache",
    "client:build": "rimraf client_build && cross-env NODE_ENV=production parcel build src/client/index.html --no-source-maps --out-dir client_build",
    "client:build:report": "yarn client:build && opn report.html",
    "server": "cross-env NODE_ENV=development node-dev -r ts-node/register --inspect=9228 src/server/index.ts",
    "server:build": "rimraf server_build && cross-env NODE_ENV=production tsc -p src/server/tsconfig.json",
    "server:prod": "cross-env NODE_ENV=production node server_build",
    "server:build:run": "yarn server:build && yarn server:prod",
    "test": "cross-env NODE_ENV=development jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:coverage": "cross-env NODE_ENV=development jest --coverage",
    "test:coverage:all": "cross-env NODE_ENV=development COVERAGE=all jest --coverage",
    "test:report": "open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.1.7",
    "colyseus": "^0.9.9",
    "colyseus.js": "^0.9.5",
    "express": "^4.16.3",
    "matter-js": "^0.14.1",
    "mousetrap": "^1.6.1",
    "pixi.js": "^5.0.0-alpha.2",
    "ramda": "^0.25.0",
    "uws": "^10.148.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/matter-js": "^0.10.0",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^10.1.2",
    "@types/ramda": "^0.25.29",
    "@types/uws": "^0.13.2",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "jest": "^22.4.4",
    "node-dev": "^3.1.3",
    "node-sass": "^4.9.0",
    "opn-cli": "^3.1.0",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-bundle-visualiser": "^1.1.2",
    "parcel-plugin-compress": "^1.0.3",
    "parcel-plugin-typescript": "^0.7.1",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.5",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">= 10.2.1",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.7.0"
  }
}
